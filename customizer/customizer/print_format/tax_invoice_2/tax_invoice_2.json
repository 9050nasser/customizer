 "html": "<style>\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tfont-family: Monospace;\n\t\tline-height: 200%;\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 5in;\n\t\t\tpadding: 0.25in;\n\t\t\tmin-height: 8in;\n\t\t}\n\t.ltr #test {direction:ltr !important;}\n    .rtl #test {direction:rtl !important;}\n    .phone {direction:ltr; display:inline-block}\n    .table {\n        table-layout: fixed;\n      }\n     .td {\n        word-wrap: break-word;\n      }\n</style>\n<h2 class=\"text-center\" style=\"direction: rtl;\">\n\u0634\u0631\u0643\u0629 \u0648\u0627\u062d\u0629 \u0627\u0644\u062d\u064a\u0648\u0627\u0646 \u0644\u0644\u062a\u062c\u0627\u0631\u0629<br>\n\u0627\u0644\u0645\u0631\u0643\u0632 \u0627\u0644\u0631\u0626\u064a\u0633\u064a - \u0627\u0644\u0631\u064a\u0627\u0636 \u062d\u064a \u0627\u0644\u0645\u0644\u0632 \u0634\u0627\u0631\u0639 \u0627\u0644\u0627\u062d\u0633\u0627\u0621<br>\n\u0645\u0642\u0627\u0628\u0644 \u0634\u0631\u0643\u0629 \u0627\u0644\u062d\u0645\u0631\u0627\u0646\u064a \u0644\u0644\u0633\u064a\u0627\u0631\u0627\u062a<br>\n<br>\n</h2>\n{% if doc.is_return %}\n\t<h2 class=\"text-center\" style=\"direction: rtl;\">\n\t\u0645\u0631\u062c\u0639 \n\t</h2><br>\n{% endif %}\n<div class=\"rtl\">\n    <b>{{ _(\"\u0627\u0644\u0634\u0631\u0643\u0629\") }}:</b> {{ doc.company }}<br>\n    <b>{{ _(\"\u0631\u0642\u0645 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629\") }}:</b> {{ doc.sales_invoice_id }}<br>\n    <b>{{ _(\"\u0627\u0644\u0628\u0644\u062f\") }}:</b> <p1 class=\"country\">\u0627\u0644\u0645\u0645\u0644\u0643\u0629 \u0627\u0644\u0639\u0631\u0628\u064a\u0629 \u0627\u0644\u0633\u0639\u0648\u062f\u064a\u0629</p1><br>\n    <b>{{ _(\"\u0627\u0644\u0645\u062f\u064a\u0646\u0629\") }}:</b> <p1 class=\"city\">  \u0627\u0644\u0631\u064a\u0627\u0636</p1><br>\n\t<b>{{ _(\"\u0647\u0627\u062a\u0641\") }}:</b> <p1 class=\"phone\">0554435085</p1><br>\n\t<b>{{ _(\"\u0631\u0642\u0645 \u0627\u0644\u0645\u0628\u0646\u064a\") }}:</b><p1 class=\"Builiding No\">{{doc.building_no}}</p1><br>\n\t{% if doc.contact_mobile %}\n\t<b>{{ _(\"\u0631\u0642\u0645 \u0647\u0627\u062a\u0641 \u0627\u0644\u0639\u0645\u064a\u0644\") }}:</b> <p1 class=\"mobile_no\">{{doc.contact_mobile}}</p1><br>\n\t{% endif %}\n\t{% if doc.is_return %}\n\t<b>{{ _(\" \u0645\u0631\u062c\u0639\u0629 \u0645\u0646 \") }}:</b> {{ doc.return_against }}<br>\n    {% endif %}\n\t<b>{{ _(\"\u0627\u0644\u062a\u0627\u0631\u064a\u062e\") }}:</b> {{ doc.get_formatted(\"posting_date\") }}<br>\n\t{% set u = frappe.get_doc(\"User\", frappe.user) %} \n\t<b>{{ _(\"\u0645\u0648\u0638\u0641\") }}:</b> <p1>{{u.full_name}}</p1><br>\n\t<b>{{ _(\"\u0627\u0644\u0639\u0645\u064a\u0644\") }}:</b> {{ doc.customer_name }}<br>\n\t<!--<b>{{(\"\u0627\u0644\u0631\u0645\u0632 \u0627\u0644\u0628\u0631\u064a\u062f\u064a \u0644\u0644\u0639\u0645\u064a\u0644\")}}:</b> <br>-->\n\t{% if doc.tax_id %}\n\t<b>{{ _(\"\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a \u0644\u0644\u0639\u0645\u064a\u0644\") }}:</b> {{ doc.tax_id }}<br>\n\t{% endif %}\n\t{% if doc.sales_person %}\n\t<b>{{ _(\"\u0645\u0633\u0624\u0644 \u0627\u0644\u0628\u064a\u0639 \") }}:</b>{{doc.sales_person}}<br>\n\t{% endif %}\n    <b>{{ _(\"\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a\") }}:</b>300334376500003<br> \n</div>\n<hr>\n<table class=\"table table-condensed cart no-border\" > \n\t<thead>\n\t\t<tr>\n\t\t\t<th width=\"24%\">{{ _(\"\u0627\u0644\u0635\u0646\u0641\") }} </th>\n            <th width=\"10%\" class=\"text-right\">{{ _(\"\u0633\u0639\u0631 \u0627\u0644\u0648\u062d\u062f\u0629\u202b\u202c\")}}</th>\t\t\n            <th width=\"9%\" class=\"text-right\">{{ _(\"\u0627\u0644\u0643\u0645\u064a\u0629\") }}</th>\n            <th width=\"15%\" class=\"text-right\">{{ _(\"\u0627\u0644\u0645\u0628\u0644\u063a \u0627\u0644\u062e\u0627\u0636\u0639 \u0644\u0644\u0636\u0631\u064a\u0628