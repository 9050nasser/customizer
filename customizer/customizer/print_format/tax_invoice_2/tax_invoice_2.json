\" style=\"width: 70%\">\n\t\t\t\t\t{{ _(\"\u0645\u062c\u0645\u0648\u0639 \u0642\u0628\u0644 \u0627\u0644\u0636\u0631\u064a\u0628\u0629\") }}\n\t\t\t\t</td>\n\t\t\t\t<td class=\"text-right\">\n\t\t\t\t\t{{ doc.get_formatted(\"net_total\", doc) }}\n\t\t\t\t</td>\n\t\t\t{% else %}\n\t\t\t\t<td class=\"text-right\" style=\"width: 70%\">\n\t\t\t\t\t<b>{{ _(\"\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a(\u063a\u064a\u0631 \u0634\u0627\u0645\u0644 \u0636\u0631\u064a\u0628\u0629 \u0627\u0644\u0642\u064a\u0645\u0629 \u0627\u0644\u0645\u0636\u0627\u0641\u0629)\" ) }}  </b>\n\t\t\t\t</td>\n\t\t\t\t<td class=\"text-right\">\n\t\t\t\t\t{{ doc.get_formatted(\"total\", doc) }}\n\t\t\t\t</td>\n\t\t\t{% endif %}\n\t\t</tr>\n\t\t{%- if doc.discount_amount -%}\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 75%\">\n\t\t\t\t{{ _(\"\u0627\u0644\u062e\u0635\u0645\") }}\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ doc.get_formatted(\"discount_amount\") }}\n\t\t\t</td>\n\t\t</tr>\n\t\t{%- endif -%}\n\t\t{%- for row in doc.taxes -%}\n\t        {%- if not row.included_in_print_rate or doc.flags.show_inclusive_tax_in_print -%}\n\t\t\t<tr>\n\t\t\t\t<td class=\"text-right\" style=\"width: 70%\">\n\t\t\t\t\t<b>{{ row.description }}</b>\n\t\t\t\t</td>\n\t\t\t\t<td class=\"text-right\">\n\t\t\t\t\t{{ row.get_formatted(\"tax_amount\", doc) }}\n\t\t\t\t</td>\n\t\t\t<tr>\n\t\t    {%- endif -%}\n\t\t{%- endfor -%}\n\n        <tr>\n\t\t\t<td class=\"text-right\" style=\"width: 75%\">\n\t\t\t\t<b>{{ _(\" \u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a (\u0627\u0644\u062e\u0627\u0636\u0639 \u0644\u0644\u0636\u0631\u064a\u0628\u0629)\") }}</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ doc.get_formatted(\"total\") }}\n\t\t\t</td>\n\t\t</tr>\n\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 75%\">\n\t\t\t\t<b>{{ _(\"\u0627\u0644\u0627\u062c\u0645\u0627\u0644\u064a\") }}</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ doc.get_formatted(\"grand_total\") }}\n\t\t\t</td>\n\t\t</tr>\n\t\t{%- if doc.rounded_total -%}\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 75%\">\n\t\t\t\t<b>{{ _(\"\u0627\u0644\u062a\u0642\u0631\u064a\u0628\") }}</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ doc.get_formatted(\"rounded_total\") }}\n\t\t\t</td>\n\t\t</tr>\n\t\t{%- endif -%}\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 75%\">\n\t\t\t\t<b>{{ _(\"\u0627\u0644\u0645\u0628\u0644\u063a \u0627\u0644\u0645\u062f\u0641\u0648\u0639\") }}</b>\n\t\t\t</td>\n\t\t\t<td class=\"text-right\">\n\t\t\t\t{{ frappe.utils.fmt_money(doc.paid_amount -  doc.loyalty_amount,2,doc.currency) }}\n\t\t\t</td>\n\t\t</tr>\n\t\t{%- if doc.change_amount -%}\n\t\t\t<tr>\n\t\t\t\t<td class=\"text-right\" style=\"width: 75%\">\n\t\t\t\t\t<b>{{ _(\"\u0627\u0644\u0628\u0627\u0642\u064a\") }}</b>\n\t\t\t\t</td>\n\t\t\t\t<td class=\"text-right\">\n\t\t\t\t\t{{ doc.get_formatted(\"change_amount\") }}\n\t\t\t\t</td>\n\t\t\t</tr>\n\t\t{%- endif -%}\n\t\t<!--{%- if doc.pos_total_qty -%}-->\n\t\t<!--<tr>-->\n\t\t<!--\t<td class=\"text-right\" style=\"width: 75%\">-->\n\t\t<!--\t\t<b>{{ _(\"\u0639\u062f\u062f \u0627\u0644\u0627\u0635\u0646\u0627\u0641\") }}</b>-->\n\t\t<!--\t</td>-->\n\t\t<!--\t<td class=\"text-right\">-->\n\t\t<!--\t\t{{ doc.get_formatted(\"pos_total_qty\") }}-->\n\t\t<!--\t</td>-->\n\t\t<!--</tr>-->\n\t\t<!--{%- endif -%}-->\n\t</tbody>\n</table>\n<hr>\n<div class=\"rtl\">{{ doc.terms or \"\" }}</div>\n\n<br><br><br><br><br><br><br><br>\n\n<p class=\"text-center\">http://petoasis.com.sa<br>\ninfo@petoasis.com.sa<br>\n\u0634\u0643\u0631\u0627 \u0644\u0643\u0645</p>\n<div style=\"min-height: 160px;\n    position: relative;\">\n<div id=\"qrcode\" class=\"text-center\" style=\"  \n    max-width: 150px;\n    position: absolute;\n    top: 80%;\n    left: 50%;\n    transform: translate(-50%, -86%);\"></div>\n</div>\n\n\n<script type=\"text/javascript\" src=\"https://cdn.rawgit.com/davidshimjs/qrcodejs/gh-pages/qrcode.min.js\"></script>\n<script type=\"text/javascript\">   \nvar currentLocation = window.location;\n\nnew QRCode(document.getElementById(\"qrcode\"), window.location.protocol + \"//\" + window.location.hostname+\"/invoice_details?i={{doc.name}}&return={{doc.is_return}}&time={{ doc.posting_time }}&date={{ doc.posting_date }}&comp={{ doc.company }}&tax_id={{ frappe.db.get_value('Company', doc.company, 'tax_id') }}&awt={{frappe.utils.fmt_money(doc.total)}}&tax_amount={{frappe.utils.fmt_money(doc.total_taxes_and_charges)}}&total_amount={{frappe.utils.fmt_money(doc.grand_total)}}\");\n</script>\n",
